<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://xmlns.jcp.org/xml/ns.javaee"
    xsi:shemaLication="http://xmlns.jcp.org/xml/ns/javaee
        http://xmls.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
    version="3.1">

    <sevlet>
        <servlet-name>mysqlDriverLoader</servlet-name>
        <servlet-class>jdbc.MySQLDriverLoad</servlet-class>
        <load-on-startup>1</load-on-startup>
    </sevlet>
    <sevlet>
        <servlet-name>JDBCInit</servlet-name>
        <servlet-class>jdbc.JDBCInit</servlet-class>
        <load-on-startup>1</load-on-startup>
    </sevlet>
</web-app>